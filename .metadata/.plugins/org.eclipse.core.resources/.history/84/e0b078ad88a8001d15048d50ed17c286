package dataStructuresExamples;



class F {
	
	// logic 1
	
	 static int fact1(int n) {
		 
		int  fact =1;
		 
		 for(int i=0;i<=n;i++) {
			 
			 fact =fact*n;
		 
		 return fact;
		 
	 }
	 
	 
}
