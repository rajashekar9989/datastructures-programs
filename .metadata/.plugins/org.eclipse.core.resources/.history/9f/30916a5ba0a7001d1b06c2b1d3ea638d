package dataStructuresExamples;

 class Demo {
	int x,y;
	Demo(){
		
		this.x  =0;
		this.y=0;
	}
	
	Demo(int x, int y){
		
		this.x =x;
		this.y =y;
	}
	
	void show() {
		
		System.out.println("X ="+this.x +" "+"Y ="+this.y) ;
	}

}

 public class Test{
	
	
	public static void main(String[] args) {
		Demo d = new Demo(111,222);
		Demo d1 = new Demo(100,200);
		Demo d2 = new Demo(300,400);
		
		d.show();
		d.show();
		d.show();
	}
}
