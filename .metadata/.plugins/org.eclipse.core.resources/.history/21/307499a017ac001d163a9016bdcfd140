package functionalInterfacesDemos;

import java.util.function.Consumer;

public class ConsumerInterDemo {

	public static void main(String[] args) {
	
		
		Consumer<Movie> c1 = m->System.out.println(m.name+"Ready to release");
		
		Consumer<Movie> c2 =m->System.out.println(m.name+"released but it is biggest flop");
		
		Consumer<Movie> c2 =m->System.out.println(m.name+"released but it is biggest flop");
		
		Consumer<Movie> c2 =m->System.out.println(m.name+"storing i nformation in database");
		
		Movie m = new Movie("spider");
		
		c1.accept(m);
		
		

	}

}
