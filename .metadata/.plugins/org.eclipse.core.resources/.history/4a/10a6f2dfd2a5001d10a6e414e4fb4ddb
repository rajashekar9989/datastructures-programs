package ListInterface;

import java.util.Stack;

public class StackDemo {
	
	// stack is child class of vactor
	
	// it is special designed class for Lats In First Out(LIFO) ORDER
	
	//1. CONSTRUCTOR OF STACK
	//  Stack s = new Stack();
	
	
	/* Methods of stack
	 * 1.Object push(Object obj);
	 * -
	 * For inserting an object to the stack
	
	 * 2. Object pop();
	 * 
	 * -To removes and returns top of  the stack.
	 * 
	 * 3. Object peek();
	 * 
	 * -To returns the top of the stack without removal of an object
	 * 
	 * 4.int search (Object 0)
	 * 
	 * -if the specified object is available it returns its offset from top of the stack .
	 * 
	 * -if the object is not available then it returns -1.
	 * 
	 * 
	 */
	  

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	Stack s = new Stack();
	
	s.push("A");
	s.push("B");
	s.push("C");
	System.out.println(s);// [A,B,C]
	
	System.out.println(s.search("A"));//3
	
	System.out.println(s.search("z"));// -1
	
	System.out.println(s.peek());
	
	
	
	
		
	}

}
